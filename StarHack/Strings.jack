class Strings {

    static Array sectorNames;
    
    function void init() {
        var int i;
        var String s;
        let sectorNames = Array.new(64);
        while (i < 64) {
            let s = String.new(8);
            do s.setInt(i);
            let sectorNames[i] = s;
            let i = i + 1;
        }
        return;
    }

    function String sectorName(int i) {
        return sectorNames[i];
    }

}