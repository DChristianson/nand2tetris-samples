class Anomaly {

    field int sectorIndex;
    field int subsectorIndex;
    field int strength;
    field int disposition;

    method void init(int sector, int subsector, int str, int dis) {
        let sectorIndex = sector;
        let subsectorIndex = subsector;
        let strength = str;
        let disposition = dis;
        return;
    }

    method int sectorIndex() {
        return sectorIndex;
    }

    method int subsectorIndex() {
        return subsectorIndex;
    }
    
}